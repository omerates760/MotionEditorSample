<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1500">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/imageView3"
               motion:framePosition="0"
               android:rotation="180" />
           <KeyPosition
               motion:motionTarget="@+id/imageView6"
               motion:framePosition="10"
               motion:percentX="0.444"
               motion:percentY="-0.047" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView6"
               motion:framePosition="0"
               android:alpha="0.0" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView6"
               motion:framePosition="0"
               android:rotation="180" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView4"
               motion:framePosition="0"
               android:alpha="0.0" />
           <KeyPosition
               motion:motionTarget="@+id/textView2"
               motion:framePosition="85"
               motion:keyPositionType="deltaRelative"
               motion:percentX="0"
               motion:percentY="0" />
           <KeyPosition
               motion:motionTarget="@+id/textView3"
               motion:framePosition="85"
               motion:percentX="0"
               motion:percentY="0"
               motion:keyPositionType="deltaRelative" />
           <KeyAttribute
               motion:motionTarget="@+id/textView2"
               motion:framePosition="85"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/textView3"
               motion:framePosition="85"
               android:alpha="0" />
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="80dp"
            android:layout_width="94dp"
            android:id="@+id/imageView3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.408"
            motion:layout_constraintHorizontal_bias="0.400" />
        <Constraint
            android:layout_height="140dp"
            android:layout_width="35dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView4"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.990"
            motion:layout_constraintHorizontal_bias="0.450" />
        <Constraint
            android:layout_height="130dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="130dp"
            android:id="@+id/imageView6"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.03"
            motion:layout_constraintHorizontal_bias="0.056" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.650"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="1.0" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintVertical_bias="0.650"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="80dp"
            android:layout_width="94dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.408"
            motion:layout_constraintHorizontal_bias="0.400" />
        <Constraint
            android:id="@+id/imageView4"
            android:layout_width="35dp"
            android:layout_height="140dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView3"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.100"
            motion:layout_constraintHorizontal_bias="0.450" />
        <Constraint
            android:layout_height="130dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="130dp"
            android:id="@+id/imageView6"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.50"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.600" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.650"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.625" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintVertical_bias="0.650"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.365" />
    </ConstraintSet>
</MotionScene>